{
	"name": "tanstack-start-example-basic",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"dev": "bun convex dev --once && concurrently -r bun:dev:web bun:dev:db",
		"dev:web": "vite dev",
		"dev:db": "convex dev",
		"build": "vite build && tsc --noEmit",
		"start": "node .output/server/index.mjs",
		"lint": "biome check .",
		"lint:fix": "biome check . --write",
		"format": "pnpm run biome format . --write && pnpm run biome check . --write --unsafe"
	},
	"dependencies": {
		"@convex-dev/react-query": "^0.0.0-alpha.11",
		"@tanstack/react-query": "^5.90.2",
		"@tanstack/react-devtools": "^0.6.9",
		"@tanstack/react-query-devtools": "5.85.5",
		"@tanstack/react-router": "^1.132.0",
		"@tanstack/react-router-devtools": "^1.132.0",
		"@tanstack/react-router-with-query": "^1.130.17",
		"@tanstack/react-start": "^1.132.1",
		"concurrently": "^8.2.2",
		"convex": "^1.27.3",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-hot-toast": "^2.5.1",
		"tailwind-merge": "^2.6.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@types/node": "^22.5.4",
		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"@vitejs/plugin-react": "^4.6.0",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.5.1",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.7.2",
		"vite": "^7.1.1",
		"vite-tsconfig-paths": "^5.1.4"
	}
}
